{
  "manifest_version": 2,
  "name": "Quality Health Associates of North Dakota - Screen Sharing Extension",
  "description": "Screen Sharing Extension for BigBlueButton",
  "version": "0.1.1",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "logo-16x16.png",
    "48": "logo-48x48.png",
    "128": "logo-128x128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://bbb.qualityhealthnd.org/*"
      ]
  },
  "web_accessible_resources": [
    "icon.png"
  ]
}
